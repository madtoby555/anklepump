<!--subpage/设置.wxml-->
<toptips msg ="{{error}}" type = "error" show ="{{error}}"></toptips>
<navigator class="weui-cell weui-cell_active weui-cell_access weui-cell_link"url="javascript:">
    <text class="weui-cell__bd">每日提醒？</text>
	<switch></switch>
</navigator>
<navigator class="weui-cell weui-cell_active weui-cell_access weui-cell_link"url="javascript:">
    <text class="weui-cell__bd">提醒时间</text>
    <picker mode="time" value="{{time}}" start="09:01"end="21:01"bindchange="bindTimeChange">
    <view class="picker">当前选择: {{time}}</view>
    </picker>
</navigator>
<navigator class="weui-cell weui-cell_active weui-cell_access weui-cell_link" url="javascript:">
					<text class="weui-cell__bd" bindtap="cleancache">清理缓存</text>

</navigator>
<navigator class="weui-cell weui-cell_active weui-cell_access weui-cell_link" url="javascript:">
					<text class="weui-cell__bd" bindtap="unregister">注销登录信息</text>

</navigator>